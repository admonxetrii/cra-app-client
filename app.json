{
  "expo": {
    "name": "Pycemon Order",
    "slug": "cra-app-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/craIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "width": "100%",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSExceptionDomains": {
          "http://192.168.0.112:8000/api": "NSTemporaryExceptionAllowsInsecureHTTPLoads"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.admonxetrii.craappclient"
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
